<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">
    <application
        android:allowBackup="true"
        android:label="RoomScan Pro"
        android:supportsRtl="true"
        android:theme="@style/Theme.RoomScanPro">
        <activity android:name=".ui.CreateRoomActivity" android:exported="false" />
        <activity android:name=".ui.ReadRoomsActivity" android:exported="false" />
        <activity android:name=".ui.UpdateRoomActivity" android:exported="false" />
        <activity android:name=".ui.DeleteRoomActivity" android:exported="false" />
        <activity android:name=".ui.MainActivity" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>

